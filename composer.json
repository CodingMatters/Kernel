{
    "name": "codingmatters/kernel",
    "description": "Core abstraction of Objects and Functions",
    "keywords": ["core"],
    "homepage": "https://chronicles.codingmatters.io/kernel/",
    "license": "MIT",
    "authors": [
        {
            "name": "Gab Amba",
            "email": "gamba@gabbydgab.com",
            "homepage": "http://www.gabbydgab.com/"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "roave/security-advisories": "dev-master",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^1.3",
        "zendframework/zend-stratigility": "^1.2",
        "zendframework/zend-expressive-template": "^1.0",
        "zendframework/zend-expressive-router": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "squizlabs/php_codesniffer": "^2.6",
        "zendframework/zend-servicemanager": "^2.7 || ^3.0",    
        "ocramius/proxy-manager": "^1.0 || ^2.0",
        "phpbench/phpbench": "^0.11",
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "CodingMatters\\Kernel\\": "src/Kernel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodingMatters\\KernelTest\\": "test/KernelTest/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "minimum-stability": "stable"
}